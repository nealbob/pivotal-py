load testdf data/data.csv
   names ["my", "name", "is", "neal", "hughes"]
   header 0

load testdf3 "data/data.csv"
   
df testdf4 from testdf

df gocats from testdf3
   filter price < 200 and quantity > 1
   select product, price, quantity, id
   set newcol = price * quantity

df testdf5 from testdf3
   select product, price, quantity
   set newcol = price * quantity
      where price < 200

df df6 from testdf3
   sort price desc, quantity asc
   left merge gocats on id

df df7 from testdf3
   left merge gocats 
      left_on id
      right_on id

df df7 from testdf3
   pivot
      agg sum price
      rows product
      cols category

df df8 from testdf3
   pivot
      agg sum price, mean quantity
      rows product
      cols category

df df9 from testdf3
   group by category
      agg mean price as mean_price, sum quantity as sum_quantity

df df10 from testdf3
   pivot
      agg sum price
      rows product

df df11 from testdf3
   select product as prod, price as cost

df df12 from testdf3
   order by price desc

df df13 from testdf3
   python print("Executing python code")
   python df13['new_col'] = 100

df df14 from testdf3
   python
      print("Multiline python start")
      df14['multi_col'] = 200
      print("Multiline python end")

df df15 from testdf3
   python varlist = ["product", "price"]
   select :varlist

df df16 from testdf3
   python single_col = "quantity"
   select product, :single_col




