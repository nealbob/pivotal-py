load testdf data/data.csv
   names ["my", "name", "is", "neal", "hughes"]
   header 0

load testdf3 "data/data.csv"
   
table testdf4 from testdf

table gocats from testdf3
   filter price < 200 and quantity > 1
   select product, price, quantity, id
   set newcol = price * quantity

table testdf5 from testdf3
   select product, price, quantity
   set newcol = price * quantity
      where price < 200

table df6 from testdf3
   sort price desc, quantity asc
   left merge gocats on id

table df7 from testdf3
   left merge gocats 
      left_on id
      right_on id

table df7 from testdf3
   pivot on price
      rows product
      cols category
      agg sum

table df8 from testdf3
   pivot on price, quantity
      rows product
      cols category
      agg sum, mean

table df9 from testdf3
   groupby category
      agg price mean, quantity sum




